    "    plt.plot(recalls2, precisions2 ,label='sci-kit learn')\n",
    "    plt.xlabel('Recall')\n",
    "    plt.ylabel('Precision')\n",
    "    plt.title('Precision-Recall Diagram')\n",
    "    plt.legend()\n",
